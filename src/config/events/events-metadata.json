{
    "Events":
	[
		{
			"Name": "awshub-job-events-int-pub-{0}",
			"Arn": "arn:aws:events:us-east-1:123456:rule/awshub-{0}-job-events-int-pub",
			"EventPattern": 
				{
					"source": ["aws.glue"],
					"detail-type": ["Job State Change"],
